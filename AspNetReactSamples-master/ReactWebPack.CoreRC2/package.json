    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-cli": "1.0.0",
    "karma-mocha": "1.0.1",
    "karma-mocha-reporter": "2.0.3",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.4.5",
    "react-hot-loader": "1.2.7",
    "react-addons-test-utils": "15.0.2",
    "expect": "1.19.0",
    "enzyme": "2.3.0",
    "json-loader": "0.5.4",

    "inject-loader": "2.0.1",
    "redux-mock-store": "1.1.1",
    "react-dnd-test-backend": "1.0.2" 
  }
